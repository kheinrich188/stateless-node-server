{
  "name": "stateless-node-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "NODE_ENV=dev nodemon",
    "build": "tsc",
    "postgres:migraton": "typeorm migration:generate -n _name_Refactoring",
    "test": "NODE_ENV=test mocha -r ts-node/register test/**/*.spec.ts test/**/**/*.spec.ts --exit",
    "github:test": "mocha -r ts-node/register test/**/*.spec.ts test/**/**/*.spec.ts --exclude test/repository/**/*.spec.ts --exit",
    "start-cloud-instance": "node -r ts-node/register ./external/cloud-instance.server.ts",
    "start-user-client": "node -r ts-node/register ./external/user.client.ts",
    "coverage": "nyc npm run test",
    "github:coverage": "nyc npm run github:test"
  },
  "author": "Konstantin Heinrich",
  "dependencies": {
    "bent": "^7.3.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.26.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "sequelize": "^5.21.12",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "typeorm": "^0.2.25",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/bent": "^7.0.2",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.153",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.23",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
